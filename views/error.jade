  .clearfix {
  *zoom: 1;
  }
  .clearfix:before,
  .clearfix:after {
  display: table;
  line-height: 0;
  content: "";
  }
  .clearfix:after {
  clear: both;
  }
  .menu {
  position: relative;
  color: white;
  border-top: 1px solid #d2dbe3;
  background-color: #fff;
  padding-right: 15px;
  padding-left: 15px;
  }
  .menu label {
  position: relative;
  cursor: pointer;
  }
  .menu span {
  position: relative;
  width: 2em;
  height: 2em;
  background: rgba(255, 255, 255, .3);
  -webkit-transition: all .3s ease;
  }
  .menu span:hover {
  background: rgba(255, 255, 255, .6);
  }
  .menu span:before,
  .menu span:after {
  content: '';
  position: absolute;
  width: 2em;
  height: .5em;
  top: 4px;
  left: 0;
  background: black;
  }
  .menu span:after {
  top: 14px;
  }
  #open {
  display: none;
  }
  nav>a {
  position: relative;
  display: inline-block;
  padding: 1em 1.5em;
  font-size: 1em;
  text-decoration: none;
  white-space: nowrap;
  color: #8a95a6;
  }
  nav>a:hover {
  background: rgba(255, 255, 255, .9);
  color: black;
  }
  .hidden-desktop {
  display: none;
  }
